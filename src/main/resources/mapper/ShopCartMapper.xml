<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atxiaoming.mapper.ShopCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.atxiaoming.entity.ShopCart">
        <result column="id" property="id" />
        <result column="cus_id" property="cusId" />
        <result column="prod_id" property="prodId" />
        <result column="prod_sku_id" property="prodSkuId" />
        <result column="quantity" property="quantity" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        cus_id, prod_id, prod_sku_id, quantity
    </sql>

</mapper>
