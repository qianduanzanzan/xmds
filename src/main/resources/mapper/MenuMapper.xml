<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atxiaoming.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.atxiaoming.entity.Menu">
        <result column="id" property="id" />
        <result column="menu_name" property="menuName" />
        <result column="route_name" property="routeName" />
        <result column="file_path" property="filePath" />
        <result column="create_at" property="createAt" />
        <result column="update_at" property="updateAt" />
        <result column="stop_flag" property="stopFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        menu_name, route_name, file_path, create_at, update_at, stop_flag
    </sql>

</mapper>
